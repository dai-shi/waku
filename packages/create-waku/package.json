{
  "name": "create-waku",
  "version": "0.6.7",
  "author": "Daishi Kato",
  "type": "module",
  "contributors": [
    "Vasu Singh"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/dai-shi/waku.git"
  },
  "bin": {
    "create-waku": "./dist/cli.js"
  },
  "files": [
    "src",
    "dist",
    "template"
  ],
  "scripts": {
    "start": "node dist/cli.js",
    "compile": "rm -rf template dist *.tsbuildinfo && pnpm run template && pnpm run build",
    "template": "cp -r ../../examples template/ && rm -rf template/*/dist && rm -rf template/*/node_modules",
    "build": "tsc -b"
  },
  "dependencies": {
    "fs-extra": "^11.2.0",
    "kolorist": "^1.8.0",
    "prompts": "^2.4.2"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/prompts": "^2.4.9"
  }
}
