{
  "name": "waku",
  "description": "⛩️ The minimal React framework",
  "version": "0.19.1",
  "type": "module",
  "author": "Daishi Kato",
  "homepage": "https://waku.gg",
  "keywords": [
    "react",
    "minimal",
    "framework"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/dai-shi/waku.git",
    "directory": "packages/waku"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/main.d.ts",
      "default": "./dist/main.js"
    },
    "./config": {
      "types": "./dist/config.d.ts",
      "default": "./dist/config.js"
    },
    "./prd": {
      "types": "./dist/prd.d.ts",
      "default": "./dist/prd.js"
    },
    "./dev": {
      "types": "./dist/dev.d.ts",
      "default": "./dist/dev.js"
    },
    "./node-loader": {
      "types": "./dist/node-loader.d.ts",
      "default": "./dist/node-loader.js"
    },
    "./client": {
      "types": "./dist/client.d.ts",
      "default": "./dist/client.js"
    },
    "./server": {
      "types": "./dist/server.d.ts",
      "default": "./dist/server.js"
    },
    "./router/client": {
      "types": "./dist/router/client.d.ts",
      "default": "./dist/router/client.js"
    },
    "./router/server": {
      "types": "./dist/router/server.d.ts",
      "default": "./dist/router/server.js"
    }
  },
  "bin": {
    "waku": "./cli.js"
  },
  "files": [
    "src",
    "dist"
  ],
  "scripts": {
    "dev": "swc src -d dist -w --strip-leading-paths",
    "compile": "rm -rf dist *.tsbuildinfo && pnpm run compile:code && pnpm run compile:types && cp ../../README.md .",
    "compile:code": "swc src -d dist --strip-leading-paths",
    "compile:types": "tsc --project tsconfig.json"
  },
  "license": "MIT",
  "engines": {
    "node": "^20.8.0 || ^18.16.0"
  },
  "dependencies": {
    "@hono/node-server": "1.5.0",
    "@swc/core": "1.3.106",
    "@vitejs/plugin-react": "4.2.1",
    "hono": "3.12.8",
    "vite": "5.0.12"
  },
  "devDependencies": {
    "@netlify/functions": "^2.5.1",
    "@swc/cli": "^0.3.2",
    "rollup": "^4.9.6"
  },
  "peerDependencies": {
    "react": "18.3.0-canary-b30030471-20240117",
    "react-dom": "18.3.0-canary-b30030471-20240117",
    "react-server-dom-webpack": "18.3.0-canary-b30030471-20240117"
  }
}
